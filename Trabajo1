//prototipos: la cabeza de las funciones

using namespace std;
//programa principal
void main()

{//tipo de dato
		float p,pf;
		do{
			cout<<" Ingresar el precio: "<<endl;
			cin>>p;
		}while(p<0);

		pf= p*(0.13+1);
		cout<<"El precio final es: "<<pf <<endl;

		getch();

}

